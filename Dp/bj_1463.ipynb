   "source": [
    "N = int(input())\n",
    "\n",
    "dp = [0]*(N+1)\n",
    "\n",
    "for i in range(2,N+1):\n",
    "    dp[i] = dp[i-1]+1\n",
    "    if i%2 == 0:\n",
    "        dp[i] = min(dp[i],dp[i//2]+1)\n",
    "    if i%3 == 0:\n",
    "        dp[i] = min(dp[i],dp[i//3]+1)\n",
    "\n",
    "print(dp[N])"
   ]
