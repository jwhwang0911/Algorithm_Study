    "    N = int(input())\n",
    "    result = 0\n",
    "    \n",
    "    while N>= 0:\n",
    "        if N % 5 == 0:\n",
    "            result += N//5\n",
    "            print(result)\n",
    "            break\n",
    "        N -= 3\n",
    "        result += 1\n",
    "    else:\n",
    "        print(-1)\n",
